#!/usr/bin/env cbmbasic
10 Z=0: L=179: R=201: B=-109: T=-63
11 FOR M=FLOOR(SQR(L)/2) TO R: X=Z: U=M
12  X=X+U: U=U-1: IF X<L THENIF U THEN 12
13  IF X>R THEN 19
14  FOR N=B TO -B: X=Z: Y=Z: U=M: V=N
15   Y=Y+V: V=V-1: X=X+U: IF U THEN U=U-1
16   IF X<L OR Y>T THENIF X<=R THENIF Y>=B THEN 15
17   IF X>=L THENIF X<=R THENIF Y>=B THENIF Y<=T THEN A=A+1
18  NEXT N
19 NEXT M: PRINT "17:";B*(B+1)/2;A
